{
  "client": "mysql",
  "version": "5.5",
  "connection": {
    "host": "localhost",
    "port": 3306,
    "user": "auth-service",
    "password": "password",
    "database": "auth-service",
    "timezone": "UTC"
  },
  "pool": {
    "min": 10,
    "max": 10,
    "acquireTimeoutMillis": 30000,
    "createTimeoutMillis": 30000,
    "destroyTimeoutMillis": 5000,
    "idleTimeoutMillis": 30000,
    "reapIntervalMillis": 1000,
    "createRetryIntervalMillis": 200
  },
  "migrations": {
    "tableName": "auth-service",
    "loadExtensions": [".ts"]
  },
  "seeds": {
    "loadExtensions": [".ts"]
  }
}